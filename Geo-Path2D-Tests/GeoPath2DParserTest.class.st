"
A GeoPath2DParserTest is a test class for testing the behavior of GeoPath2DParser
"
Class {
	#name : #GeoPath2DParserTest,
	#superclass : #TestCase,
	#category : #'Geo-Path2D-Tests'
}

{ #category : #tests }
GeoPath2DParserTest >> test_001 [

	| parser document d |
	d := 'M 10 10 H 90 V 90 H 10 Z'.
	parser := GeoPath2DParser on: d.
	
	document := parser parseDocument.
	self assert: document d equals: d
]

{ #category : #tests }
GeoPath2DParserTest >> test_002 [

	| parser document d |
	d := 'M 10 10 h 80 v 80 h -80 Z'.
	parser := GeoPath2DParser on: d.
	
	document := parser parseDocument.
	self assert: document d equals: d
]

